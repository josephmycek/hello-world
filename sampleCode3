function squarecube() {    
    alert("Numbers, their Squares and their Cubes" + "\n"
        + "5:  25  125" + "\n"
        + "6:  36  216" + "\n"
        + "7:  49  343" + "\n"
        + "8:  64  512" + "\n"
        + "9:  81  729" + "\n"
        + "10:  100  1000" + "\n"
        + "11:  121  1331" + "\n"
        + "12:  144  1728" + "\n"
        + "13:  169  2197" + "\n"
        + "14:  196  2744" + "\n"
        + "15:  225  3375" + "\n");
    }
